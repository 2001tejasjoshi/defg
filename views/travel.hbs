    <div class='container mt-3' style='min-height: 50vh'>
      <div id='alertPlaceholder'></div>
      <form id='start' action='/' method='POST'>
        <fieldset>
          <legend>Travel Form</legend>

          <div class='form-group'>
            <label for='exampleSelect1' class='form-label mt-4'>Choose Starting
              Point</label>
            <select
              class='form-select'
              id='exampleSelect1'
              name='source'
              required
            >
              <option id='0000' disabled selected>Select</option>
              <option id='1000'>Jayanagara 4th Block</option>
              <option id='2000'>Sanjay Gandhi Hospital</option>
              <option id='3000'>Carmel Convent</option>
              <option id='4000'>Pump House</option>
              <option id='5000'>East End Circle</option>
            </select>
          </div>
          <br />

          <div class='form-group'>
            <label for='exampleSelect2' class='form-label mt-4'>Choose
              Destination Point</label>
            <select
              class='form-select'
              id='exampleSelect2'
              name='dest'
              required
            >
              <option id='0001' disabled selected>Select</option>
              <option id='1001'>Jayanagara 4th Block</option>
              <option id='2001'>Sanjay Gandhi Hospital</option>
              <option id='3001'>Carmel Convent</option>
              <option id='4001'>Pump House</option>
              <option id='5001'>East End Circle</option>
            </select>
            <input type='hidden' value='user' name='user' />
          </div>
          <br />
          <button type='submit' id='submit' class='btn btn-primary'>
            Submit
          </button>
        </fieldset>
      </form>
      <br />
      <br />
      <br />
    </div>
